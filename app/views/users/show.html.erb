<p id="notice"><%= notice %></p>
<% if current_user %>
	<%= render 'info' %>
	<% if current_user.admin? %>
		<%= link_to 'Transactions', user_transactions_path(@user) %> 
		<%= link_to 'Edit', edit_user_path(@user) %> |
	<% end %>
<% else %>
	<%= render 'layouts/sign_in_sign_up' %>
<% end %>
<%= link_to 'Back', users_path %>